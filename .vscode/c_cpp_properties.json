{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include"
        ],
        "myCompilerPath": "/usr/local/bin/"
    },
    "configurations": [
        {
            "name": "Mac",
            "intelliSenseMode": "macos-gcc-x64",
            "includePath": [
                "${myDefaultIncludePath}",
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**"
            ],
            "defines": [
                "BAR=100"
            ],
            "forcedInclude": [
                "${workspaceFolder}/include/units.h"
            ],
            "compilerPath": "/usr/local/bin/gcc-10",
            "cStandard": "gnu17",
            "cppStandard": "c++14",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}